HARBOR=edp2harbor.psa.com.sg
NS=edplivevns
CLUSTER=edplivevkc
cat > new-secret-harbor-ca.yaml <<EOF
apiVersion: v1
data:
 additional-ca-1: $(curl -sSk https://${HARBOR}/api/v2.0/systeminfo/getcert | base64 -w0 | base64 -w0) 
kind: Secret
metadata:
 name: ${CLUSTER}-user-trusted-ca-secret
 namespace: $NS
type: Opaque
EOF
kubectl apply -f new-secret-harbor-ca.yaml
kubectl -n $NS get secrets ${CLUSTER}-user-trusted-ca-secret -oyaml

