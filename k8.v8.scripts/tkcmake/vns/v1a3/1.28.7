apiVersion: run.tanzu.vmware.com/v1alpha3
kind: TanzuKubernetesCluster
metadata:
  name: "287"
  namespace: vns
spec:
  topology:
    controlPlane:
      replicas: 1
      vmClass: best-effort-xsmall
      storageClass: storageclass
      tkr:  
        reference:
          # This name MUST match your "kubectl get tkr" NAME column exactly
          name: v1.28.7---vmware.1-fips.1-tkg.1
    nodePools:
      - name: node-pool-1
        replicas: 1
        vmClass: best-effort-xsmall
        storageClass: storageclass
        tkr:
          reference:
            name: v1.28.7---vmware.1-fips.1-tkg.1